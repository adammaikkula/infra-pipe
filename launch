stage ("CreateInstance") {
    sh "aws ec2 run-instances --image-id ami-287dnf7 --count 1 --instance-type t2.micro --key-name SEIS665-03-Spring19-VA --security-group-ids sg-047314ec5ddc6d6fb --subnet-id subnet-subnet-201f2d6a --region us-east-1"

}

stage ("GetInstances") {

    sh "aws ec2 describe-instances --region us-east-1"
}
